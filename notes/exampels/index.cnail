let a = "Hello World!"
let b = ""
for (0, 12 | i: int → nil) {
	b = b + a[11 - i]
}
print(b)

let c = ["Hello", " ", "World", "!"]
b = ""
for (0, 4 | i: int → nil) {
	b = b + c[i]
}
print(b)

let d = [["Hello", "World", ","], ["Hello", "World", "!"]]
b = ""
for (0, 2 | i: int -> nil) {
	for (0, 3 | j: int -> nil) {
		b = b + d[i][j] + " "
	}
}
print(b)
